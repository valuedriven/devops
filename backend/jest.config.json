{    
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/*.js",
      "**/src/*/*.js",
      "!**/src/server.js",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!**/builds/**",
      "!**/src/test/**"
    ],  
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
